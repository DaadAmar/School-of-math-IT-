#include <iostream>
using namespace std;
int main() {
  cout<<"\n                COMPARING ARRAYS! \n";
  int K[5][5]={{1,44,665,21,45},{2,354,6,223,0},{99,56,4,2,1},{3,56,33,12,6},{45,6,78,2,12}},B[5]={100,23,4,5,78};
  cout<<"\nThe two dimenstional array:\n\n";
for (int i=0;i<=4;i++)
    {
      for (int j=0;j<=4;j++)
        {
         
         cout<<K[i][j]<<"  ";
        }
      cout<<endl;
    }
  cout<<endl;
  cout<<"The one dimenstion array:\n\n";
for (int j=0;j<=4;j++)
        {
         
         cout<<B[j]<<"  ";
        }
      cout<<endl;
for (int i=0;i<=4;i++)
    {
      for (int j=0;j<=4;j++)
        {
         if (B[j] < K[i][j])
         {
           B[j]=K[i][j];
         }
        }
      }
  cout<<endl;
  cout<<"The out come of the comparison!\n\n";
for (int j=0;j<=4;j++)
        {
         
         cout<<B[j]<<"  ";
        }
      cout<<endl;
  }
